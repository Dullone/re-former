<% @user.errors.full_messages.each do |msg| %>
  <%= "Error: " + msg %><br/>
<% end %>
<br/>
<%= form_for @user, url:{ action: "update" } do |f| %>
Name    : <%= f.text_field :name %> <br/>
email   : <%= f.text_field :email %><br/>
password: <%= f.password_field :password %><br/>
<%= f.submit %>
<% end %>
